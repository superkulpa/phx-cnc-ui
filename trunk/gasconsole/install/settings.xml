	CXExitDialog QAbstractButton { padding: 15px 15px; }
	CXVirtualKeyboard CXTouchButton { min-width: 40px; min-height: 40px; }
	CXUtilsWindow QAbstractButton { min-width: 35px; min-height: 35px; }
