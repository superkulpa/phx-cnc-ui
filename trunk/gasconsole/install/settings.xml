	#frameSpacerWidget { min-width: 30px; }
	#mVelocityView {  min-width: 80px; max-width: 120px; }
	CXOperDirectionWindow #xyButton, CXOperDirectionWindow #plusButton, CXOperDirectionWindow #minusButton { min-height: 55px; min-width: 55px; font-size: 12pt; }
