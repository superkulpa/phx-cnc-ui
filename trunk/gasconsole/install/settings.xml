  CXSupportsWindow QAbstractButton { min-width: 25px; min-height: 35px; }
	
	QScrollBar:horizontal { height: 40px; }
	QScrollBar:vertical { width: 40px; }
	QScrollBar::sub-line:vertical { height: 30px; }
